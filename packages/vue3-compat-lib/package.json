{
  "name": "vue3-compat-lib",
  "version": "0.1.0",
  "private": false,
  "description": "A library achieving partial runtime compat between Vue 2 and Vue 3 components",
  "author": "Thorsten LÃ¼nborg <t.luneborg@googlemail.com>",
  "homepage": "",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./vue2": {
      "import": "./dist/vue2/index.es.js",
      "require": "./dist/vue2/index.umd.js"
    },
    "./": "./"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:v3 && yarn build:v2 && yarn build:types",
    "build:v3": "vite build",
    "build:v2": "cross-env BUILD_TARGET_V2=1 vite build",
    "build:types": "rollup --config ../../config/rollup.types.config.js",
    "typecheck": "vuedx-typecheck src",
    "typecheck:watch": "vuedx-typecheck  src --watch",
    "lint": "eslint 'src/**/*.{ts,vue}'",
    "test": "jest --config ../../jest.config.ts --runInBand",
    "test:w": "jest --config ../../jest.config.ts --watch --runInBand"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}

{
  "name": "vue3-compat-lib",
  "version": "0.1.0",
  "private": false,
  "description": "A library achieving partial runtime compat between Vue 2 and Vue 3 components",
  "author": "Thorsten LÃ¼nborg <t.luneborg@googlemail.com>",
  "homepage": "",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./": "./"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && yarn build:types",
    "build:types": "rollup --config ../../config/rollup.types.config.js",
    "typecheck": "vuedx-typecheck src",
    "typecheck:watch": "vuedx-typecheck  src --watch",
    "lint": "eslint 'src/**/*.{ts,vue}'",
    "test": "jest --config ../../jest.config.ts --runInBand",
    "test:w": "jest --config ../../jest.config.ts --watch --runInBand"
  },
  "dependencies": {
    "tslib": "^2.0.0",
    "vue-demi": "^0.10.0"
  },
  "devDependencies": {
    "@vue/composition-api": "^1.0.0-rc.13",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.13",
    "vue": "^2.0.0 || >=3.0.4"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}

{
  "name": "@vue-bridge/vite-plugin",
  "version": "0.0.1",
  "description": "A Vite Plugin to build Vue libraries for both Vue 2 and Vue 3",
  "author": "Thorsten Luenborg",
  "repository": {
    "type": "git",
    "url": "https://github.com/LinusBorg/vue-bridge.git",
    "directory": "packages/vite-plugin"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup-node index.ts --format esm,cjs --dts",
    "build:w": "pnpm build -- --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.23",
    "vite-plugin-vue2": "^1.8.0",
    "vue-demi": "^0.12.1",
    "vue-template-compiler": "^2.6.14"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "debug": "^4.3.3",
    "resolve-pkg": "^2.0.0",
    "tsup": "^5.11.9",
    "vite": "^2.7.9",
    "vue": "^3.2.23"
  },
  "peerDependencies": {
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.1.4",
    "vite-plugin-vue2": "^1.8.0",
    "vue": "^3.2.0 || ^2.6.x",
    "vue-demi": "^0.11.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "vue-demi": {
      "optional": true
    },
    "@vitejs/plugin-vue": {
      "optional": true
    },
    "@vue/compiler-sfc": {
      "optional": true
    },
    "vite-plugin-vue2": {
      "optional": true
    },
    "vue-template-compiler": {
      "optional": true
    }
  }
}
